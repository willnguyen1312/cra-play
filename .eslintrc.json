{
    "extends": [
        "airbnb",
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "rules": {
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "trailingComma": "es5",
            "semi": false
        }],
        "semi": "off"
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    }
}